<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="EME - Smoke Testing">
    <listeners>
		<listener class-name = "EME.listeners"/>
	<listener class-name = "resources.ExtentReporterNG"/>
	</listeners> 
  <test thread-count="5" name="Member Login">
    <classes>
      <class name="EME.MemberLoginTest"/>
    </classes>
  </test>
    <test thread-count="5" name="Page Launch">
    <classes>
      <class name="EME.PageLaunchTest"/>
    </classes>
  </test> 
  <test thread-count="5" name="Make Payment">
    <classes>
      <class name="EME.MakePaymentTest"/>
    </classes>
  </test> 
  <test thread-count="5" name="Make Appointment">
    <classes>
      <class name="EME.MakeApptTest"/>
    </classes>
  </test> 
</suite>
